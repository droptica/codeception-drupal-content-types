{
  "name": "ixis/codeception-drupal-content-types",
  "description": "A Codeception module to provide Drupal content types support.",
  "authors": [
    {
      "name": "Chris Cohen",
      "email": "chris.cohen@ixis.co.uk"
    },
    {
      "name": "Andy Rigby",
      "email": "andy.rigby@ixis.co.uk"
    }
  ],
  "require": {
    "chriscohen/codeception-drupal-pages": "dev-master"
  },
  {
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
  },
  "autoload": {
    "psr-4": {
      "Codeception\\Module\\": "src",
      "Codeception\\Module\\Drupal\\ContentTypeRegistry\\": "src/Drupal/ContentTypeRegistry",
      "Codeception\\Module\\Drupal\\ContentTypeRegistry\\Fields\\": "src/Drupal/ContentTypeRegistry/Fields",
      "Codeception\\Module\\Drupal\\ContentTypeRegistry\\Widgets\\": "src/Drupal/ContentTypeRegistry/Widgets"
    }
  }
}
